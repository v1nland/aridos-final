{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "7.1",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-mbstring": "*",
    "ext-mcrypt": "*",
    "ext-pdo_mysql": "*",
    "ext-soap": "*",
    "ext-xml": "*",
    "barryvdh/laravel-dompdf": "^0.8.2",
    "fideloper/proxy": "~3.0",
    "laravel/framework": "5.5.*",
    "laravel/scout": "3.0",
    "laravel/socialite": "^3.0",
    "laravel/tinker": "~1.0",
    "league/flysystem-aws-s3-v3": "^1.0",
    "maatwebsite/excel": "^2.1",
    "nategood/httpful": "^0.2.20",
    "predis/predis": "*",
    "rollbar/rollbar-laravel": "2.4.1",
    "siturra/doctrine1-php7": "^1.2",
    "siturra/tcpdf-laravel": "^5.5",
    "tamayo/laravel-scout-elastic": "^3.0",
    "vladimir-yuldashev/laravel-queue-rabbitmq": "^6.0.0"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0",
    "symfony/thanks": "^1.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Helpers/helpers.php",
      "app/Helpers/JsonHelper.php",
      "app/Helpers/JWTHelper.php",
      "app/Helpers/SaferEval.php",
      "app/Helpers/dateHelper.php",
      "app/Helpers/nusoap/lib/nusoap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
        "php": "7.1"
    }
  }
}
